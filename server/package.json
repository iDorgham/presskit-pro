{
  "name": "presskit-pro-server",
  "version": "1.0.0",
  "description": "Backend API server for PressKit Pro",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "nodemon --exec ts-node server.ts",
    "build": "tsc",
    "test": "jest",
    "lint": "eslint src/**/*.ts app.ts server.ts",
    "lint:fix": "eslint src/**/*.ts app.ts server.ts --fix",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@sentry/node": "^9.40.0",
    "axios": "^1.10.0",
    "bcryptjs": "^3.0.2",
    "cloudinary": "^2.7.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.0",
    "express": "^5.1.0",
    "express-rate-limit": "^8.0.1",
    "express-validator": "^7.2.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.16.4",
    "multer": "^2.0.2",
    "nodemailer": "^7.0.5",
    "stripe": "^18.3.0",
    "winston": "^3.17.0"
